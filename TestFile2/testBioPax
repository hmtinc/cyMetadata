 <bp:displayName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">MMP2_HUMAN</bp:displayName>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Matrix metalloproteinase-2</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Gelatinase A</bp:name>
