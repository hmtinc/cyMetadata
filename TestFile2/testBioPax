 <bp:displayName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">T53I1_HUMAN</bp:displayName>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Stress-induced protein</bp:name>
