<bp:ProteinReference rdf:about="http://identifiers.org/uniprot/Q12837">
 <bp:standardName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">POU domain, class 4, transcription factor 2</bp:standardName>
 <bp:xref rdf:resource="#UnificationXref_uniprot_knowledgebase_Q12837" />
 <bp:xref rdf:resource="#RelationshipXref_hgnc_symbol_POU4F2_identity" />
 <bp:organism rdf:resource="http://identifiers.org/taxonomy/9606" />
 <bp:displayName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">PO4F2_HUMAN</bp:displayName>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Brain-3B</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">POU4F2</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">BRN3B</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Brn-3B</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Brain-specific homeobox/POU domain protein 3B</bp:name>
