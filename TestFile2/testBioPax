 <bp:entityFeature rdf:resource="#ModificationFeature_25619c42961095059bbd310163e1166b" />
 <bp:standardName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Transcription activator BRG1</bp:standardName>
 <bp:xref rdf:resource="#UnificationXref_uniprot_knowledgebase_P51532" />
 <bp:xref rdf:resource="#RelationshipXref_hgnc_symbol_SMARCA4_identity" />
 <bp:organism rdf:resource="http://identifiers.org/taxonomy/9606" />
 <bp:displayName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">SMCA4_HUMAN</bp:displayName>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">SNF2L4</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Protein brahma homolog 1</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">BAF190A</bp:name>
