 <bp:xref rdf:resource="#RelationshipXref_hgnc_symbol_PRMT1_identity" />
 <bp:xref rdf:resource="#UnificationXref_uniprot_knowledgebase_Q99873" />
 <bp:organism rdf:resource="http://identifiers.org/taxonomy/9606" />
 <bp:displayName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">ANM1_HUMAN</bp:displayName>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Histone-arginine N-methyltransferase PRMT1</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">PRMT1</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">2.1.1.319</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">HMT2</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">IR1B4</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">HRMT1L2</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Interferon receptor 1-bound protein 4</bp:name>
 <bp:comment rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">REPLACED http://identifiers.org/uniprot.isoform/Q99873-4</bp:comment>
 <bp:comment rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">REPLACED http://pathwaycommons.org/pc2/ProteinReference_HPRD_04257_identity</bp:comment>
