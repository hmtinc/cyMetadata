 <bp:displayName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">P53_HUMAN</bp:displayName>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">P53</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Tumor suppressor p53</bp:name>
